						custom deluge script
						(
											fetchmemtype = Member_rules[ID == input.Member_Type];
										if(fetchmemtype.count(ID) > 0)
										{
											input.Member_Fees = fetchmemtype.Member_fees;
											input.Member_book_count = fetchmemtype.Member_book_count;
											input.No_of_days_allowed = fetchmemtype.Allowed_days;
										}
										else
										{
											alert "No data found";
											input.Member_Fees = null;
										}
						)
					}
				}

			}
			add_member_details as "add member details"
			{
				type =  form
