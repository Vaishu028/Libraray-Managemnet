		workflow
		{
		form
		{
			genarate_book_id as "genarate book id"
			{
				type =  form
				form = Book_details
				record event = on add

				on success
				{
					actions 
					{
						custom deluge script
						(
											// input.Book_Id1 = "B00" + input.Auto_Number;
										// if(input.Member_Type != 0)
										// {
										// 	planame = input.Member_Type.Member_type;
										// 	firstch = planame.subString(0,1).toUpperCase();
										// 	memtype = Member_details[Member_Type.Member_type == planame];
										// 	coun = memtype.count();
										// 	input.Member_Id = firstch + coun;
										// }
										if(input.Book_name != null)
										{
											bookname = input.Book_name;
											firstch = bookname.subString(0,1).toUpperCase();
											bookname1 = Book_details[Book_name == bookname];
											coun = bookname1.count();
											input.Book_Id1 = firstch + coun;
										}
						)
					}
				}

			}
			hide_fields as "hide fields"
			{
				type =  form
				form = Book_details
				record event = on add or edit

				field rules
				{
					actions 
					{
						hide Book_Id1;
					}
					actions 
					{
						disable Book_available;
					}
				}

			}
			Book_quantity as "Book quantity"
			{
				type =  form
				form = Book_details
				record event = on add or edit

				on user input of Book_quantity
				{
					actions 
					{
						custom deluge script
						(
											if(input.Book_quantity < 0)
										{
											alert "Enter a valid number";
											input.Book_quantity = null;
										}
						)
					}
				}

			}
			generate_member_id as "generate member id"
			{
				type =  form
				form = Member_details
				record event = on add

				on success
				{
					actions 
					{
						custom deluge script
						(
											// coun = Member_details.count();
										// input.Member_Id = input.Member_Type.Member_type.substring(0,1).toUpperCase() + "00" + coun;
										if(input.Member_Type != 0)
										{
											planame = input.Member_Type.Member_type;
											firstch = planame.subString(0,1).toUpperCase();
											memtype = Member_details[Member_Type.Member_type == planame];
											coun = memtype.count();
											input.Member_Id = firstch + coun;
										}
						)
					}
				}

			}
			disable_fields as "disable fields"
			{
				type =  form
				form = Member_details
				record event = on add or edit

				field rules
				{
					actions 
					{
						disable Member_Fees,Member_book_count,Member_Id;
