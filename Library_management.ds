				record event = on add or edit

				on user input of Member_ID
				{
					actions 
					{
						custom deluge script
						(
											var = Member_details[ID == input.Member_ID];
										input.Member_name = var.Member_Name;
										input.Member_type = var.Member_Type.Member_type;
										input.Address = var.Member_Address;
										input.Phone_Number = var.Member_Phone;
										input.Member_book_count = var.Member_book_count;
										input.no_of_days_allowed = var.No_of_days_allowed;
										if(input.Member_type.equalsIgnoreCase("silver"))
										{
											input.Return_Date = input.issue_date.addDay(4);
										}
										else if(input.Member_type.equalsIgnoreCase("gold"))
										{
											input.Return_Date = input.issue_date.addDay(7);
										}
										else if(input.Member_type.equalsIgnoreCase("platinum"))
										{
											input.Return_Date = input.issue_date.addDay(10);
										}
						)
					}
				}

			}
