			generate_book_issue_id as "generate book issue id"
			{
				type =  form
				form = Book_issue
				record event = on add or edit

				on success
				{
					actions 
					{
						custom deluge script
						(
											input.Book_issue_id = "BIS" + input.Auto_Number;
						)
					}
				}

			}
			final_book_Return as "final_book_Return"
			{
				type =  form
				form = Book_return1
				record event = on add or edit

				on user input of Book_issue_id
				{
					actions 
					{
						custom deluge script
						(
											if(Book_issue_id != null)
										{
											var = Book_issue[ID == input.Book_issue_id];
											input.Member_name = var.Member_name;
											input.Member_id = var.Member_ID;
											input.Member_type = var.Member_type;
											input.Book_id = var.Book_id;
											input.Book_name = var.Book_name.Book_name;
											input.Book_issue_Date = var.issue_date;
											input.Book_tbr_date = var.Return_Date;
										}
										else
										{
											input.Member_name = null;
											input.Member_id = null;
											input.Member_type = null;
											input.Book_id = null;
											input.Book_name = null;
											input.Book_issue_Date = null;
											input.Book_tbr_date = null;
										}
										dayslate = input.Book_tbr_date.daysBetween(input.Current_Date);
										if(dayslate > 0)
										{
											input.Days_late = dayslate;
										}
										else
										{
											input.Days_late = 0;
										}
						)
					}
				}

			}
			book_status_workflow as "book_status_workflow"
			{
				type =  form
				form = Book_return1
				record event = on add or edit

				on user input of Book_status
				{
					actions 
					{
						custom deluge script
						(
											amount = 10;
										var = Book_details[Book_name == input.Book_name];
										if(input.Book_status.equalsIgnoreCase("Damage"))
										{
											input.Fine_amount = var.Book_cost / 2;
										}
										else if(input.Book_status.equalsIgnoreCase("Over due"))
										{
											input.Fine_amount = input.Days_late.toNumber() * amount;
										}
										else if(input.Book_status.equalsIgnoreCase("Lost"))
										{
											input.Fine_amount = var.Book_cost;
										}
										else
										{
											input.Fine_amount = 0;
										}
										input.Total_fine_amount = input.Fine_amount;
						)
					}
				}

			}
			member_book_count as "member book count"
			{
				type =  form
				form = Book_issue
				record event = on add or edit

				on success
